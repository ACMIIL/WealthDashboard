    <link href="~/css/wpregistrationcss/slick.css" rel="stylesheet" />
    <link href="~/css/wpregistrationcss/slick-theme.css" rel="stylesheet" />

    <!-- Template Main CSS File -->
    <link href="~/css/wpregistrationcss/style.css" rel="stylesheet" />
    <link href="~/css/wpregistrationcss/bootstrap.css" rel="stylesheet" />
    <link href="~/css/wpregistrationcss/custom.css" rel="stylesheet" />



    <script>
        function blockspace(event) {
            // Get the key code of the pressed key
            var keyCode = event.keyCode || event.which;

            // Check if the pressed key is the backspace key (key code 8)
            if (keyCode === 32) {
                // Prevent the default behavior of the backspace key
                event.preventDefault();
                return false;
            }
            return true;



        }
    </script>
</head>
<body>

    <div>


        <!-- ======= Hero Section ======= -->
        <section id="hero" class=" background-twc-banner">
            <div class="container">
                <header id="header" class="pb-5">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12 text-center mb-4">
                                <a href="index.html">
                                    <img src="~/wpregimages/twc-logo.png" alt="" title="" class="img-fluid">
                                </a>
                            </div>
                        </div>
                    </div>
                </header>
                <div class="row">
                    <div class="col-md-12 mx-auto">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="row">
                                    <div class="mx-auto">
                                        <img src="~/wpregimages/twc-mobile.png" class="img-fluid">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-7">
                                <div class="row ">
                                    <div class="col-md-12 ms-0 mx-auto card login-form">
                                        <div class="row mx-auto mt-5 mb-2">
                                            <img src="~/wpregimages/form-logo.png" alt="" title=""
                                                 class="img-fluid text-center" style="width:55px;height: 31px;">
                                        </div>
                                        <div class="row mb-1">
                                            <div class="form-title text-center">
                                                Personal Details
                                            </div>
                                            <div class="text-center">
                                            </div>
                                        </div>
                                        <div>
                                            <div class="row mb-4">
                                                <div class="col-md-6 ps-4">
                                                    <div class="form-floating mb-2">
                                                        <input type="text"  ng-model="aadharNo"
                                                               maxlength="12" 
                                                               class="form-control floating-text-area" id="floatingInput"
                                                               style="color: white;" placeholder="HERFK4578G">
                                                        <label for="floatingInput" style="color: #CAAA86;">Aadhar No</label>

                                                        

                                                    </div>



                                                    <div class="form-floating mb-2">
                                                        <input type="text"  ng-model="email"
                                                               class="form-control floating-text-area" id="floatingInput"
                                                               style="color: white;" placeholder="HERFK4578G">
                                                        <label for="floatingInput" style="color: #CAAA86;">Email</label>

                                                     

                                                    </div>



                                                    <div class="form-floating mb-2">
                                                        <input type="text"  ng-model="pincode" ng-change="getStateCity($event)"
                                                               class="form-control  floating-text-area" id="floatingInput"
                                                               style="color: white;" placeholder="HERFK4578G">
                                                       
                                                        <label for="floatingInput" style="color: #CAAA86;">Pincode</label>
                                                    </div>


                                                    <div class="form-floating mb-2">
                                                        <input type="text" ng-model="city"
                                                               class="form-control  floating-text-area" id="floatingInput"
                                                               style="color: white;" placeholder="HERFK4578G">

                                                        <label for="floatingInput" style="color: #CAAA86;">City</label>
                                                    </div>


                                                    <div class="form-floating mb-2">
                                                        <input type="text" ng-model="state"
                                                               class="form-control  floating-text-area" id="floatingInput"
                                                               style="color: white;" placeholder="HERFK4578G">

                                                        <label for="floatingInput" style="color: #CAAA86;">State</label>
                                                    </div>

                                                    <div class="form-floating mb-2">
                                                        <input type="text"  ng-model="country"
                                                               class="form-control  floating-text-area" id="floatingInput"
                                                               style="color: white;" placeholder="HERFK4578G">

                                                        <label for="floatingInput" style="color: #CAAA86;">Country</label>
                                                    </div>


                                                </div>

                                                <div class="col-md-6 ps-3">
                                                    <div class="form-floating mb-2">
                                                        <input type="text"  ng-model="address1"
                                                               class="form-control  floating-text-area" id="floatingInput"
                                                               style="color: white;" placeholder="HERFK4578G">
                                                      
                                                        <label for="floatingInput" style="color: #CAAA86;">Address 1</label>
                                                    </div>
                                                    <div class="form-floating mb-2">
                                                        <input type="text"  ng-model="address2"
                                                               class="form-control  floating-text-area" id="floatingInput"
                                                               style="color: white;" placeholder="HERFK4578G">

                                                        <label for="floatingInput" style="color: #CAAA86;">Address 2</label>
                                                    </div>

                                                    <div class="form-floating mb-2">
                                                        <input type="text"  ng-model="address3"
                                                               class="form-control  floating-text-area" id="floatingInput"
                                                               style="color: white;" placeholder="HERFK4578G">

                                                        <label for="floatingInput" style="color: #CAAA86;">Address 3</label>
                                                    </div>





                                                    <div class="form-floating mb-2">
                                                        <span>
                                                            <img src="~/wpregimages/upload-icon.png" class="date-icon">
                                                            <input type="file" hidden #gst (change)="onFileSelect(gst,'pan')" accept="image/png,image/jpeg" />
                                                        </span>
                                                        <input readonly type="text" class="form-control floating-text-area"
                                                              ng-model="pan" id="floatingInput"
                                                               style="color: white;padding-left: 0 !important;font-size: 12px !important;"
                                                               placeholder="31-12-1995">
                                                     
                                                        <label for="floatingInput"
                                                               style="color: #CAAA86;font-size: 16px;opacity: 1;">
                                                            Upload  Pan
                                                        </label>

                                                        <span ng-if="pansize" style="color: darkred;font-size:small;">
                                                            Oops! The
                                                            size limit for the image is 5 MB. Reduce the file size and try
                                                            again.
                                                        </span>
                                                    </div>

                                                    <div class="form-floating mb-3">
                                                        <span>
                                                            <img src="~/wpregimages/upload-icon.png" class="date-icon">
                                                            <input type="file" hidden #pms (change)="onFileSelect(pms,'aadhar')" accept="image/png,image/jpeg" />
                                                        </span>
                                                        <input readonly type="text" class="form-control floating-text-area" ng-model="aadhar" id="floatingInput"
                                                               style="color: white;padding-left: 0 !important;font-size: 12px !important;"
                                                               placeholder="31-12-1995">
                                                     
                                                        <label for="floatingInput"
                                                               style="color: #CAAA86;font-size: 16px;opacity: 1;">Upload  Aadhar </label>
                                                        <span ng-if="aadharsize" style="color: darkred;font-size:small;">
                                                            Oops! The
                                                            size limit for the image is 5 MB. Reduce the file size and try
                                                            again.
                                                        </span>
                                                    </div>
                                                </div>







                                            </div>
                                            <div class="row mb-3 row mb-3 ms-2">
                                                <div class="col-md-12 text-center col-12 mt-2 mb-4">
                                                    <button type="button" class="btn rounded btn-submit">Next</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </section><!-- End Hero -->
    </div>


</body>
</html>
