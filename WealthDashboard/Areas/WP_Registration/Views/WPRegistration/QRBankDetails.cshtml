@{
    Layout = "_WPLayout";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>TWP</title>
    <link rel="canonical" href="" />
    <meta content="" name="description">
    <meta content="" name="keywords">
    <!-- Favicons -->
    <link href="" rel="icon">
    <link href="" rel="apple-touch-icon">
    <!-- Google Fonts -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Template Main CSS File -->
    <link href="~/css/wpregistrationcss/style.css" rel="stylesheet" />
    <link href="~/css/wpregistrationcss/bootstrap.css" rel="stylesheet" />
    <link href="~/css/wpregistrationcss/custom.css" rel="stylesheet" />
</head>
<body>
    <!-- ======= Hero Section ======= -->
    <div ng-app="main" ng-controller="myController">
        <section class="background-twc-banner">
            <div class="container">
                <header id="header" class="pb-5 mobile-top-padding-header ">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-2 col-6 mx-auto text-center ">
                                <a href="index.html">
                                    <img src="~/wpregimages/twc-logo.png" alt="" title="" class="img-fluid">
                                </a>
                            </div>
                        </div>
                    </div>
                </header>
                <div class="row">
                    <div class="col-md-12 mx-auto">
                        <div class="row">
                            <!--mob img div -->
                            <div class="col-md-5 d-none d-md-block">
                                <div class="row">
                                    <div class="mx-auto">
                                        <img src="/wpregimages/twc-mobile.png" class="img-fluid">
                                    </div>
                                </div>
                            </div>
                            <!--End == mob img div -->
                            <div class="col-md-7">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="card login-form">
                                            <div class="row mx-auto mt-5 mb-2">
                                                <img src="/wpregimages/bank3.png" class="img-fluid">
                                            </div>
                                            <div class="row">
                                                <div class="col-md-12 text-center mt-2 mb-2">
                                                    <h5 class="form-title"> Confirm your bank account details
                                                    </h5>
                                                </div>
                                                <div class="col-md-12">
                                                    <p class="text-success text-center">
                                                        <b>
                                                            <img src="/wpregimages/icon-park-solid_success.svg"> Bank account
                                                            details fetched successfully
                                                        </b>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="row mb-3">
                                                <div class="col-md-10 mx-auto">
                                                    <label class="font-label-1" style="color: #c9a13f;">Bank Name</label>
                                                    <span class="p-0"> {{BANK}}</span>
                                                </div>
                                                <div class="col-md-10 mx-auto">
                                                    <label class="font-label-1" style="color: #c9a13f;">Account Holder Name</label>
                                                    <!-- <input type="text" class="form-control" name="password" value="Amit Ashok Kumar"> -->
                                                    <span class="p-0"> {{account_Name}}</span>
                                                </div>
                                                <div class="col-md-10 mx-auto">
                                                    <label class="font-label-1" style="color: #c9a13f;">IFSC Code</label>
                                                    <!-- <input type="text" class="form-control" name="password" value="HDFC00145754"> -->
                                                    <span class="p-0">&nbsp;&nbsp; {{IFSC}}</span>
                                                </div>
                                                <div class="col-md-10 mx-auto">
                                                    <label class="font-label-1" style="color: #c9a13f;">Account Number</label>
                                                    <!-- <input type="text" class="form-control" name="password" value="123456789"> -->
                                                    <span class="p-0">&nbsp;&nbsp;{{accountno}}</span>
                                                </div>
                                                <div class="col-md-10 mx-auto">
                                                    <h3 class="title-4" style="color: #c9a13f;">Account Type</h3>
                                                </div>
                                                <div class="col-md-10 mx-auto">
                                                    <div class="d-flex">
                                                        <label class="font-label-2" style="color: #c9a13f;">
                                                            <input type="radio" name="account"
                                                                   checked value="1"> Saving Account
                                                        </label>&nbsp;&nbsp;
                                                        <label class="font-label-2" style="color: #c9a13f;">
                                                            <input type="radio" name="account"
                                                                   value="0">
                                                            Current Account
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-12">
                                                    <div class="small text-danger text-center ">
                                                        <p>Please Select Account Type</p>
                                                    </div>
                                                </div>
                                                <div class="col-md-10 mx-auto text-center">
                                                    <button class="btn btn-primary btn-submit-popup1 btn-sm" ng-click="Gotoselfie()">
                                                        Confirm &
                                                        Continue
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
        </section><!-- End Hero -->
        <!-- ======= Footer ======= -->
        <!--<footer id="footer" class="pt-2 pb-2">
            <div class="container">
            <div class="row">
                <div class="col-12 col-md-12 text-center">
                    <a href="mailto:customerservices@acm.co.in" class="link1">customerservice@acm.co.in</a>
                </div>
            </div>
            </div>
        </footer>-->
        <!-- End Footer -->



        <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>
    </div>
    <!-- Vendor JS Files -->
    <script src="~/js/wpregistration/jquery.min.js"></script>
    <script src="~/js/wpregistration/bootstrap.bundle.min.js"></script>
    <!-- Template Main JS File -->
    <script src="~/js/wpregistration/main.js"></script>

    <!---for Angular js---->

    <script src="~/angularjs/toast/angular.min.js"></script>
    <script src="~/angularjs/toast/angular-animate.min.js"></script>
    <link href="~/angularjs/tosters/toaster.css" rel="stylesheet" />
    <script src="~/angularjs/tosters/toaster.js"></script>
    <script src="~/js/wpregistration/qrbankdetails/qrbankdetails.js"></script>

</body>

</html>

