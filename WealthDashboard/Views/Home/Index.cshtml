    <!--banner-->
    <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item" ng-class="{'active': $index === 0}" ng-repeat="item in mergedData">
                <div class="d-block w-100" ng-bind-html="item.trustedSliderImages"></div>
                <div class="carousel-caption text-start">
                    <h1 class="poppins-bold">{{ item.header }}</h1>
                    <p class="poppins-light-italic">{{ item.content }}</p>
                    <div class="outer">
                        <div class="button">
                            <a ng-href="{{item.url}}" target="_blank" class="text">
                                <span>GET STARTED</span>
                                <svg xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" width="18" height="18" x="0" y="0" viewBox="0 0 32 32">
