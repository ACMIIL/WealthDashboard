    <script>
        // Store tab elements and content in variables
        const tabs = [
            { id: 'v-pills-home-tab', content: 'Evaluate your portfolio v/s Current market' },
            { id: 'v-pills-profile-tab', content: 'Check your portfolio for risk and diversification' },
            { id: 'v-pills-messages-tab', content: 'Get all your questions answered for free in a 1-1 call' }
        ];

        let intervalId;

        // Function to switch tabs after a specified time
        function autoSwitchTabs() {
            let currentIndex = 0;
            intervalId = setInterval(function () {
                // Change the active tab programmatically
                document.getElementById(tabs[currentIndex].id).click();
                currentIndex = (currentIndex + 1) % tabs.length;
            }, 3000); // 3000 milliseconds (3 seconds)
        }

        // Function to check screen width and call autoSwitchTabs if within specified ranges
        function checkScreenWidth() {
            const width = window.innerWidth;
            if (width >= 350 && width <= 996) {
                if (!intervalId) {
                    autoSwitchTabs();
                }
            } else {
                clearInterval(intervalId);
                intervalId = null;
            }
        }

        // Call the function when the page loads
        window.onload = checkScreenWidth;

        // Add an event listener to check screen width on resize
        window.onresize = checkScreenWidth;
    </script>
